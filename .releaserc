{
  "branches": [
    "master"
  ],
  "plugins": [
    "@semantic-release/commit-analyzer",
    "@semantic-release/release-notes-generator",
    "@semantic-release/github",
    "@semantic-release/npm",
    ["@semantic-release/git", {
      "message": "chore(release): ${nextRelease.version} [skip ci]\n\nISSUE: https://github.com/MazaiSerg/fire-calculator/issues/${JSON.stringify(branch)}\n${nextRelease.notes}"
    }]
  ]
}
